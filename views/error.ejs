<% layout('layouts/boilerplate') %>
<div class="row">
    <div class="col-6 offset-3">
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading"><%= err.message %>>!</h4>
            <hr>
            <% if(process.env.NODE_ENV !== 'Production'){ %>
            <p class="mb-0"><%= err%></p>
            <% } %>
          </div>
    </div>
</div>
